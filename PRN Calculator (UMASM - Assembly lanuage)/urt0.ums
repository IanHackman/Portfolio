# urt0.ums
# A minimal runtime that initializes the stack
# and sets the zero register to 0.

.temps r6, r7
.zero r0

# Initialize the call stack
.section stk
    .space 1000
    endstack:

# Initializen the zero register and the stack pointer
.section init
    start:
        r0 := 0
        r2 := endstack
